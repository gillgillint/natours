{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css", 
    "compress:css": "node-sass css/style.concat.css  css/style.css  --output-style compressed",
    "build:css": " npm-run-all  compile:sass concat:css compress:css",

    "build-bootstrap": "node-sass -w sass/vendors/bootstrap-5/bootstrap.scss -o css/bootstrap.css"
    
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concat": "^1.0.3",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "bootstrap": "^5.0.1"
  }
}
